<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.oceaniceindia.indiane_newspapers">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TeluguPapers" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".HindiPapers" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".EnglishPapers" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".NewsPaperTelugu" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".NewsPaperHindi" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".NewsPaperEnglish" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".About" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".Feedback" android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-4221818112925027~1710633034"
            />
    </application>

</manifest>